{% trans_default_domain 'FOSUserBundle' %}

<form novalidate="novalidate" action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_widget(form) }}
    <div>
        <button type="submit" class="btn btn-default">{{ 'registration.submit'|trans }}</button>
        <a href="{{ path('fos_user_security_login') }}">{% trans from "user" %}Entrar en tu cuenta{% endtrans %}</a>
    </div>
</form>
