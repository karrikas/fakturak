{% trans_default_domain 'FOSUserBundle' %}

<form novalidate="novalidate" action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_reset">
    {{ form_widget(form) }}
    <div>
        <button type="submit" class="btn btn-default">{{ 'resetting.reset.submit'|trans }}</button>
    </div>
</form>
